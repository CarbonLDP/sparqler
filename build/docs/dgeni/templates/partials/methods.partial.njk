{% set members = doc.members | selectattr( 'parameters' ) %}
{% if members | length %}
    <h3 class="doc-content__title">Methods</h3>
    {% for member in members %}
        {{ memberCode.method( member ) }}
        {{ member.description | marked }}
        {% if not loop.last %}
            <div class="ui section divider"></div>
        {% endif %}
    {% endfor %}
{% endif %}
