{% set members = doc.members | selectattr( 'params' ) | rejectattr( 'isIndex' ) %}
{% if members | length %}
    <h3 class="doc-content__title">Methods</h3>
    {% for member in members %}
        {{ memberCode.method( member ) }}
        {{ member.description | marked }}
        {{ parameters(doc, member) }}
        {% if member.returns %}
            <h4>Returns</h4>
            {{ member.returns.description | marked }}
        {% endif %}
        {% if not loop.last %}
            <div class="ui section divider"></div>
        {% endif %}
    {% endfor %}
{% endif %}
