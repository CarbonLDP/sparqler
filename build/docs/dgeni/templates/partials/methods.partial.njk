{% set members = doc.members | selectattr( 'parameters' ) %}
{% if members | length %}
    <h3 class="doc-content__title">Methods</h3>
    {% for member in members %}
        <div class="member-code">
            <span class="member-code__hjs typescript">{{ member.name }}{{ member.generics | escape }}({{ member.parameters | join(', ') | linkify }})</span>
        </div>
        {{ member.description | marked }}
        {% if not loop.last %}
            <div class="ui section divider"></div>
        {% endif %}
    {% endfor %}
{% endif %}
