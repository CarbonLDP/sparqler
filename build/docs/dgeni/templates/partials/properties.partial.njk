{% set members = doc.members | rejectattr( 'params' ) %}
{% if members | length %}
    <h3 class="doc-content__title">Properties</h3>
    {% for member in members %}
        {{ memberCode.property( member ) }}
        {{ member.description | marked }}
        {% if not loop.last %}
            <div class="ui section divider"></div>
        {% endif %}
    {% endfor %}
{% endif %}
