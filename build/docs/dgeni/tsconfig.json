{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2016",
		"lib": [ "es2016", "dom" ],
		"noImplicitAny": false,
		"removeComments": true,
		"sourceMap": true,
		"baseUrl": ".",
		"paths": {
			"semantic-ui/*": [ "semantic-ui/*" ],
			"src/*": [ "src/*" ]
		},
		"types": [  "node", "dgeni" ]
	},
	"exclude": [
		"../../node_modules",
		"../../dist"
	]
}