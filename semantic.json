{
  "base": "build/docs/semantic-ui",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "input",
    "header",
    "icon",
    "image",
    "label",
    "list",
    "segment",
    "menu",
    "table",
    "item",
    "search",
    "divider",
    "message"
  ],
  "version": "2.2.13"
}