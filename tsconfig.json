{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "commonjs",
		"target": "es5",
		"lib": [ "es2015" ],
		"noImplicitAny": false,
		"removeComments": true,
		"baseUrl": ".",
		"paths": {
			"sparqler/*": [ "src/*" ]
		},
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"types": [ "jasmine" ]
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}